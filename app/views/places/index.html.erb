<div class="container mt-1">
  <div class="row">

    <div class="col-sm-9"> 
      <h1>Places I've Been</h1>
    </div>

    <div class="col-sm-3 text-end">
      <% if @current_user != nil %>
        <a href="/places/new"  class="btn btn-outline-primary .text-nowrap">Add a new place</a>
      <% end %>
    </div>
  </div>
</div>

<div class="row">

  <% for place in @places %>
    <% if @current_user != nil %>
    <a href="/places/<%= place["id"] %>"><%= place["name"] %></a>
    <% end %>
  <% end %>

</div>   

